find greater number of 2.
package com.Aptiquestions;

public class demo7 {

	public static void main(String[] args) {
		int a=100,b=120,c=90;
		if(a>b) {
			if(a>c) {
				System.out.println("a is greater");
			}
			else {
				System.out.println("c is greater");
			}}
			else {
				if(b>c) {
					System.out.println("b is greater");
				}
				else {
					System.out.println("c is greater");
				}
			}
		}

	

}
output=b is greater
