fing greater number.package com.Aptiquestions;

public class demo6 {

	public static void main(String[] args) {
   int a=400,b=50,c=60;
   if(a>b) {
	   if(a>c) {
		   System.out.println("a is greater");
	   }
	   else {
		   System.out.println("c is greater");
	   }}
	   else if(b>c){
		   System.out.println("b is greater");
	   }
	   else {
		   System.out.println(" c is greater");
	   }
   }}
	

output=a is greater
